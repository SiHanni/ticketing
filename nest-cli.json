      }
    },
    "reservation-service": {
      "type": "application",
      "root": "apps/reservation-service",
      "entryFile": "main",
      "sourceRoot": "apps/reservation-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/reservation-service/tsconfig.app.json"
      }
    },
    "seat-service": {
      "type": "application",
      "root": "apps/seat-service",
      "entryFile": "main",
      "sourceRoot": "apps/seat-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/seat-service/tsconfig.app.json"
      }
    },
    "user-service": {
      "type": "application",
      "root": "apps/user-service",
      "entryFile": "main",
      "sourceRoot": "apps/user-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user-service/tsconfig.app.json"
      }
    },
    "websocket-service": {
      "type": "application",
      "root": "apps/websocket-service",
